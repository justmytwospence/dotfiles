" https://github.com/1995eaton/chromium-vim

" Variables
let blacklists = ['*://localhost/notebooks*']
let completionengines = ['duckduckgo', 'google', 'google-image', 'google-maps', 'imdb', 'wikipedia', 'youtube']
let configpath = '/Users/spencerboucher/.cvimrc'
let homedirectory = '/Users/spencerboucher'
let mapleader = '<Space>'

" Mappings
map <C-b> :buffer<Space>
map <Leader>W :new<Space>
map <Leader>o :open duckduckgo<Space>
map <Leader>p :call pocket<CR>
map <Leader>s :source /Users/spencerboucher/.cvimrc<CR>
map <Leader>t :tabnew duckduckgo<Space>
map <Leader>w :tabdetach<CR>
map ZZ quitChrome
map b :bookmarks /Bookmarks Bar
map h :tabprevious<CR>
map l :tabnext<CR>
unmap J
unmap K

" Settings
set autoupdategist
set cncpcompletion
set linkanimations
set localconfig
set noautofocus
set noshowtabindices
set smoothscroll
set sortlinkhints
